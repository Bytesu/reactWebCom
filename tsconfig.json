{
  "compilerOptions": {
    "strict": false,
    "target": "es6",
    "module": "commonjs",
    "jsx": "react",
    "esModuleInterop": true,
    "importHelpers": true,//123
    "sourceMap": true,
    "allowSyntheticDefaultImports": true
  },
  "exclude": [
    "node_modules",
    "src/**/*.js",
    "src/**/*.jsx"
  ],
  "include": [
    "src/**/*.tsx"
  ]
}
